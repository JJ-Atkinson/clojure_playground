{:paths ["src/main"]

 :deps  {com.fulcrologic/guardrails     {:mvn/version "0.0.12"}
         medley                         {:mvn/version "1.3.0"}
         meander/epsilon                {:mvn/version "0.0.512"}
         quil                           {:mvn/version "3.1.0"}
